{
    "name": "muhammad-nawlo/filament-scout-manager",
    "description": "A comprehensive Filament plugin for managing Laravel Scout indexes and search settings",
    "keywords": [
        "Muhammad-Nawlo",
        "laravel",
        "filamentphp",
        "filament",
        "filament-plugin",
        "Scout",
        "Index",
        "Search",
        "filament-scout-manager"
    ],
    "homepage": "https://github.com/muhammad-nawlo/filament-scout-manager",
    "support": {
        "issues": "https://github.com/muhammad-nawlo/filament-scout-manager/issues",
        "source": "https://github.com/muhammad-nawlo/filament-scout-manager"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad-Nawlo",
            "email": "nawlomuhammadit@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "laravel/scout": "^10.24",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-settings": "^3.7"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.7|^4.0",
        "pestphp/pest-plugin-arch": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0",
        "pestphp/pest-plugin-livewire": "^3.0|^4.0",
        "rector/rector": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "MuhammadNawlo\\FilamentScoutManager\\": "src/",
            "MuhammadNawlo\\FilamentScoutManager\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadNawlo\\FilamentScoutManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test": "pest",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadNawlo\\FilamentScoutManager\\FilamentScoutManagerServiceProvider"
            ],
            "aliases": {
                "FilamentScoutManager": "MuhammadNawlo\\FilamentScoutManager\\Facades\\FilamentScoutManager"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
